<Page
    x:Class="Battleship.GameSetup"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Battleship"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid>
        <Grid.Background>
            <ImageBrush Stretch="Fill" ImageSource="backgrounds/gamesetupback.png" Opacity="0.7">
                <ImageBrush.RelativeTransform>
                    <CompositeTransform CenterY="0.5" CenterX="0.5"/>
                </ImageBrush.RelativeTransform>
            </ImageBrush>
        </Grid.Background>

        <Grid.RowDefinitions>
            <RowDefinition Height=".9*"/>
            <RowDefinition Height="1.3*"/>
            <RowDefinition Height=".7*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height=".3*"/>
            <RowDefinition Height=".31*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <TextBlock x:Name="SetupTitle"
    		HorizontalAlignment="Center"
    		Grid.Row="0"
    		Text="Game Setup"
    		VerticalAlignment="Center"
    		 
    		Style="{StaticResource TitleTextBlockStyle}" Foreground="White" Height="32" />



        <StackPanel x:Name="infogather"
    		Margin="20,10,0,0"
    		HorizontalAlignment="left"
    		VerticalAlignment="Top"
    		Grid.Row="1" Height="130" Width="250" Grid.RowSpan="2">

            <StackPanel Name="infogatherStackPanel">
                <TextBlock Name="nameLabel"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Top"
                              Text="Player Name"
                              FontSize="13.333" />

                <TextBox Name="playerName"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Top"
                         FontSize="14.667"
                         PlaceholderText="Enter Player Name"
                         Margin="0,0,0,0" Width="200" Background="White" Height="24" MaxLength="20" />

                <TextBlock Name="numShipsLabel"
                              HorizontalAlignment="Left"
                              VerticalAlignment="Top"
                              Text="Number of Ships (Between 2 and 5)"
                              FontSize="13.333" Margin="0,3,0,0"
                           />

                <TextBox Name="numOfShips"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Top"
                         FontSize="14.667"
                         PlaceholderText="2"
                         Margin="0,0,0,0" Width="50" Background="White" Height="22" MaxLength="1" />
                <TextBlock Name="messageArea"
                           HorizontalAlignment="Left"
                           VerticalAlignment="Top"
                           FontSize="14.667"
                           Foreground="Red"
                           Width="280"
                           Height="20" />

            </StackPanel>
        </StackPanel>

